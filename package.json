{
  "name": "wpcl-hd-reader",
  "version": "0.1.0",
  "description": "WPCL HD Disk reader",
  "main": "reader.js",
  "dependencies": {
    "async": "^2.5.0",
    "mongoose": "^4.11.1",
    "winston": "^2.3.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpmg/wpcl_hd_reader.git"
  },
  "devDependencies": {
    "eslint": "^4.1.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node reader.js"
  },
  "author": "Wilmer Prentius",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "indent": [
        1,
        2
      ],
      "no-console": 0,
      "no-unused-vars": 1
    }
  }
}
